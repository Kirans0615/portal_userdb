[Unit]
Description=UserDB gunicorn daemon
After=network.target

[Service]
User=bco_api_user
Group=nginx
WorkingDirectory=/var/www/bcoeditor/bco-test/server/
ExecStart=/var/www/bcoeditor/bco-test/client/env/bin/gunicorn --workers 3 --bind 127.0.0.1:8181 portalusers.wsgi:application --access-logfile /var/log/gunicorn/userdb_stdout.log --log-level=debug --log-file /var/log/gunicorn/userdb_stderr.log

[Install]
WantedBy=multi-user.target
